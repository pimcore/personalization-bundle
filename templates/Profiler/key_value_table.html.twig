<table class="{{ class|default('') }}">
    <thead>
    <tr>
        <th scope="col" class="key">{{ labels is defined ? labels[0] : 'Key' }}</th>
        <th scope="col">{{ labels is defined ? labels[1] : 'Value' }}</th>
    </tr>
    </thead>
    <tbody>
    {% for key in data|keys %}
        <tr>
            <th>{{ key }}</th>
            {# @see https://talk.pimcore.org/t/profiler-error-when-viewing-targeting-tab/4097/2 #}
            <td>
                {% if data[key] is iterable %}
                    {{ profiler_dump(data[key], maxDepth=maxDepth|default(0)) }}
                {% else %}
                    {{ data[key] }}
                {% endif %}
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="2">(no data)</td>
        </tr>
    {% endfor %}
    </tbody>
</table>